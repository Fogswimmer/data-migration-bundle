{
  "name": "grokhotov/data-migration",
  "type": "symfony-bundle",
  "description": "Declarative data migration between legacy databases and Symfony entities.",
  "keywords": [
    "php",
    "symfony",
    "data migration"
  ],
  "authors": [
    {
      "name": "Andrey Dyakov"
    }
  ],
  "license": "MIT",
  "require": {
    "php": "^8.2",
    "symfony/console": "^6.4 || ^7.0",
    "symfony/dependency-injection": "^6.4 || ^7.0",
    "symfony/config": "^6.4 || ^7.0",
    "symfony/yaml": "^6.4 || ^7.0",
    "doctrine/dbal": "^3.7",
    "symfony/property-access": "7.3.*",
    "doctrine/doctrine-bundle": "^2.16.2",
    "doctrine/doctrine-migrations-bundle": "^3.4.2",
    "doctrine/orm": "^3.5.2"
  },
  "require-dev": {
    "symfony/framework-bundle": "^6.4 || ^7.0",
    "doctrine/doctrine-bundle": "^2.10",
    "phpunit/phpunit": "^10.5",
    "phpstan/phpstan": "^2.1",
    "squizlabs/php_codesniffer": "^4.0",
    "friendsofphp/php-cs-fixer": "^3.92"
  },
  "autoload": {
    "psr-4": {
      "Grokhotov\\DataMigration\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Grokhotov\\DataMigration\\Tests\\": "tests/"
    }
  },
  "minimum-stability": "dev",
  "prefer-stable": true
}
